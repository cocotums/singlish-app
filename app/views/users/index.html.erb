<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<ul>

<% @users.each do |user| %>
    <% if current_user.profile_pic.attached? %>
        <%= image_tag user.profile_pic, :style => "border: 3px solid black", width: 100, height: "auto" %>
    <% end %>
    <li><%= link_to user.username, user_path(user.id) %> </li>
<% end %>

</ul>